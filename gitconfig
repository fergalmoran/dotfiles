[user]
	email = fergal.moran@gmail.com
	name = Fergal Moran

[gitflow "branch"]
        master = trunk
        develop = develop
[gitflow "prefix"]
        feature = @feature/
        bugfix = @bugfix/
        release = @release/
        hotfix = @hotfix/
        support = @support/
        versiontag = 

[init]
	defaultBranch = trunk
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process

[merge]
        tool = code-insiders
[mergetool]
        writeToTemp = true
            keepBackup = false
[mergetool "code-insiders"]
        cmd = code-insiders --wait -m $REMOTE $LOCAL $BASE $MERGED
[alias]
	ignore = "!gi() { curl -sL https://www.toptal.com/developers/gitignore/api/$@ ;}; gi"
[safe]
	directory = /opt/flutter
[pull]
	rebase = false
[push]
	autoSetupRemote = true
